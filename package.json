{
  "name": "unxlsx",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@7.1.5",
  "description": "Auto generate files from xlsx",
  "author": "sudongyuer",
  "license": "MIT",
  "keywords": [
    "xlsx",
    "generate"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "require": "./dist/utils.js",
      "import": "./dist/utils.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "unxlsx": "bin/unxlsx.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "tscbuild": "rimraf ./dist && tsc",
    "watch": "watch 'pnpm run tscbuild' ",
    "dev": "unbuild --stub",
    "build": "pnpm run tscbuild",
    "test": "vitest",
    "changeset": "changeset add",
    "version": "changeset version",
    "release": "pnpm run build && pnpm run release:only",
    "release:only": "changeset publish",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --cache",
    "lint-fix": "eslint . --fix",
    "try": "tsx src/index.ts"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "fs-extra": "^10.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@changesets/cli": "^2.23.0",
    "@sudongyuer/eslint-config": "^0.1.3",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.0.0",
    "eslint": "^8.19.0",
    "rimraf": "^3.0.2",
    "tsx": "^3.6.0",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.4",
    "unconfig": "^0.3.5",
    "vite": "^2.9.12",
    "vitest": "^0.15.2",
    "watch": "^1.0.2"
  }
}
